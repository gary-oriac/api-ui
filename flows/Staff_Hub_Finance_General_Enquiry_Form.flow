            <name>Type_of_request</name>
            <choiceReferences>choiceGeneralEnquiries</choiceReferences>
            <choiceReferences>choicePayments</choiceReferences>
            <choiceReferences>choiceFinanceSystems</choiceReferences>
            <choiceReferences>choiceFinanceBudgetRelated</choiceReferences>
            <dataType>String</dataType>
            <defaultSelectedChoiceReference>choiceGeneralEnquiries</defaultSelectedChoiceReference>
            <fieldText>Type of request</fieldText>
            <fieldType>DropdownBox</fieldType>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>Subject_of_your_enquiry</name>
            <dataType>String</dataType>
            <fieldText>Subject of your enquiry</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>Your_Enquiry</name>
