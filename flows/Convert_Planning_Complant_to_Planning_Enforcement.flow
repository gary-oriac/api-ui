<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Triggered by a process builder this flow will swap the record type on the enforcement case that triggers it</description>
    <interviewLabel>Convert Planning Complant to Planning Enforcement {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Convert Planning Complant to Planning Enforcement</label>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>Lookup_Enforcement_Record_Id</name>
        <label>Lookup Enforcement Record Id</label>
        <locationX>250</locationX>
        <locationY>485</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Set_record_Type_to_Enforcement</targetReference>
        </connector>
        <filters>
            <field>DeveloperName</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>Enforcement</stringValue>
            </value>
        </filters>
        <filters>
            <field>SobjectType</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>arcusbuiltenv__Enforcement__c</stringValue>
            </value>
        </filters>
        <object>RecordType</object>
        <outputAssignments>
            <assignToReference>PlanningEnfRTId</assignToReference>
            <field>Id</field>
        </outputAssignments>
    </recordLookups>
    <recordUpdates>
        <name>Set_record_Type_to_Enforcement</name>
        <label>Set record Type to Enforcement</label>
        <locationX>373</locationX>
        <locationY>747</locationY>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>EnformcentDetails.Id</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>RecordTypeId</field>
            <value>
                <elementReference>PlanningEnfRTId</elementReference>
            </value>
        </inputAssignments>
        <object>arcusbuiltenv__Enforcement__c</object>
    </recordUpdates>
    <startElementReference>Lookup_Enforcement_Record_Id</startElementReference>
    <status>Draft</status>
    <variables>
        <description>Detail of the record that triggered the flow</description>
        <name>EnformcentDetails</name>
        <dataType>SObject</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
        <objectType>arcusbuiltenv__Enforcement__c</objectType>
    </variables>
    <variables>
        <description>Stores the record type Id for Planning enforcement</description>
        <name>PlanningEnfRTId</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>false</isInput>
        <isOutput>false</isOutput>
    </variables>
</Flow>
