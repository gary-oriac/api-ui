    <validationRules>
        <fullName>CRM_fields_are_required_for_contacts</fullName>
        <active>true</active>
        <description>CRM Fields are made required for contacts - phone, email, address and first name. As required via CRM UAT Trello.</description>
        <errorConditionFormula>AND(
$Profile.Name = "FHDC Officer (CRM)",
OR(
ISBLANK(FirstName),
ISBLANK ( Account.Name ),
ISBLANK( Phone ),
( AND (ISBLANK( arcusgazetteer__Mailing_Address__c ), ISBLANK(  OtherAddress  )))))</errorConditionFormula>
        <errorMessage>These fields are required when creating a new contact as an officer - first and last name, associated organisation / account, phone number and either the gazetteer mailing address or other address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FHDC_Prevent_Update_of_Name</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
     ISCHANGED(Full_Name__c), 
      RecordType.Name = 'Council Officer',
    OR($Profile.Name='FHDC Officer (Backoffice)',
       $Profile.Name='FHDC Officer (CRM)'))</errorConditionFormula>
        <errorMessage>You cannot modify Contact Name</errorMessage>
    </validationRules>
